{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-proyecto-7376905"
		},
		"sa_ventas_raw_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'sa_ventas_raw'"
		},
		"sql_dw_ventas_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'sql_dw_ventas'"
		},
		"sql_dw_ventas_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "sql-proyecto-7376905.database.windows.net"
		},
		"sql_dw_ventas_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "dw-ventas"
		},
		"sql_dw_ventas_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "vanessa"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-proyecto-7376905_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"sa_ventas_raw_connectionString": {
						"value": "[parameters('sa_ventas_raw_connectionString')]"
					},
					"sql_dw_ventas_password": {
						"value": "[parameters('sql_dw_ventas_password')]"
					},
					"sql_dw_ventas_properties_typeProperties_server": {
						"value": "[parameters('sql_dw_ventas_properties_typeProperties_server')]"
					},
					"sql_dw_ventas_properties_typeProperties_database": {
						"value": "[parameters('sql_dw_ventas_properties_typeProperties_database')]"
					},
					"sql_dw_ventas_properties_typeProperties_userName": {
						"value": "[parameters('sql_dw_ventas_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "adf-proyecto-7376905_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'adf-proyecto-7376905_ArmTemplate_0')]"
			]
		},
		{
			"name": "adf-proyecto-7376905_ArmTemplate_2",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_2.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'adf-proyecto-7376905_ArmTemplate_1')]"
			]
		},
		{
			"name": "adf-proyecto-7376905_ArmTemplate_3",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_3.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'adf-proyecto-7376905_ArmTemplate_2')]"
			]
		}
	]
}